    background-color: #FFFFFF;
}

/* Cuando está desplegado */
QComboBox:on { 
    border-bottom-left-radius: 0px;  /* Quitamos curvas de abajo para conectar con la lista */
    border-bottom-right-radius: 0px;
}

/* 1. EL BOTÓN CUADRADO DE LA DERECHA */
QComboBox::drop-down {
    subcontrol-origin: padding;
    subcontrol-position: top right;
    width: 20px; /* Ancho del área clickeable */
    
    border-left-width: 0px; /* Sin línea divisoria */
    border-top-right-radius: 10px; /* Redondeamos igual que el combo */
    border-bottom-right-radius: 10px;
    background-color: transparent; /* ¡Vital! Para que no se vea un parche de color */
}

/* 2. LA IMAGEN DE LA FLECHA */
QComboBox::down-arrow {
    /* Reemplaza 'assets/flecha_abajo.png' con tu ruta real */
    image: url(C:/Users/Cachi/Desktop/WQI Calculator/resources/combobox_arrow_down.png);
	
    
    width: 10px;   /* Tamaño pequeño para elegancia */
    height: 10px;
    
    /* Centrado y márgenes */
    content: &quot;&quot;;   /* A veces ayuda a limpiar residuos */
    margin-right: 10px; /* Separación del borde derecho */
}

/* 3. (Opcional) EFECTO AL ABRIR */
/* Si tu flecha es negra, quizás no necesites cambiarla, 
   pero si quisieras rotarla o cambiarla, usarías esto */
QComboBox::down-arrow:on {
    /* Podrías poner otra imagen: url(assets/flecha_arriba.png); */
    /* O moverla ligeramente para dar efecto de &quot;click&quot; */
    top: 1px; 
    left: 1px;
}</string>
