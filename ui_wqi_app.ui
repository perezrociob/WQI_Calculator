/* 3. LOS ENCABEZADOS (HEADERS) */
QHeaderView::section {

	background-color: #FFFFFF;    
    color: #3C6E71;              
    border: 2px solid #D9D9D9;     

    border-radius: 15px;          
    margin: 1px 1px;               /* Separamos los headers entre sí (Arriba/Abajo, Izq/Der) */
    padding: 1px 1px;             /* Relleno para que el texto respire */
    
    font: 87 10pt &quot;Segoe UI Black&quot;;
}


/* 5. SCROLLBAR VERTICAL (Minimalista) */
QScrollBar:vertical {
    background: #F5F5F5;
    width: 8px;               /* Muy delgada */
    margin: 0px 0px 0px 0px;
}

QScrollBar::handle:vertical {
    background: #CCCCCC;      /* Gris suave */
    min-height: 20px;
    border-radius: 4px;       /* Redondeada */
}

QScrollBar::handle:vertical:hover {
    background: #999999;      /* Un poco más oscuro al pasar mouse */
}

/* Ocultar flechas de arriba y abajo del scroll para limpieza total */
QScrollBar::add-line:vertical, QScrollBar::sub-line:vertical {
    height: 0px;
}
QScrollBar::add-page:vertical, QScrollBar::sub-page:vertical {
    background: none;
}</string>
